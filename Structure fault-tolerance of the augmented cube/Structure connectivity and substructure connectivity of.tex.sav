According to Theorem 1, we have the following result.
\begin{thm}
For n $\geq$ 4, $\kappa(AQ_n,K_1) = \kappa^s(AQ_n,K_1)=2n-1$.
\end{thm}

%AQ_n的连通度
\begin{lem}
\label{1_extraconnectivity}
\cite{Ma2008}
For n $\geq$ 6, $\kappa_1(AQ_n)= 4n - 8$.
\end{lem}

%如果两个节点是hypercube edge,它们的共同的相邻节点数
\begin{prop}
\cite{Ma2008}
\label{hypercube_edge}
If $(u,u^i)$ is a hypercube edge of dimension $i$ $(1 \leq i\leq n)$, then\\

\[
{N_{AQ_n}(u)}\cap {N_{AQ_n}(u^i)}=\left\{
             \begin{array}{lcr}
             \{\bar{u}^i,\bar{u}^{i-1}\} & &2\leq i\leq n,\\
             \{\bar{u}^2,u^2\} & &i=1.
             \end{array}
\right.
\]
That is, $u$ and $u^i$ have exactly two common neighbors in $AQ_n$ and $|{N_{AQ_n}}(\{u,u^i\})|= 4n-6$.
\end{prop}

%如果两个节点是complement edge,它们的共同的相邻节点数
\begin{prop}
\cite{Ma2008}
\label{complement_edge}
If (u,$\bar{u}^i$) is a complement edge of dimension i $(2\leq i\leq n)$,then\\

\[
{N_{AQ_n}(u)}\cap {N_{AQ_n}(\bar{u}^i)}=\left\{
             \begin{array}{lcr}
             \{\bar{u}^{i-1},u^i,u^{i+1},\bar{u}^{i+1}\} & &2\leq i\leq n-1,\\
             \{\bar{u}^{n-1},u^n\} & &i=n.
             \end{array}
\right.
\]
That is, $u$ and $\bar{u}^i$ have exactly four common neighbors in $AQ_n$ for $2\leq i\leq n-1$ and $|{N_{AQ_n}}(\{u,\bar{u}^i\})|=4n-8$. Similarly, $u$ and $\bar{u}^n$ have exactly two common neighbors in $AQ_n$ and $|{N_{AQ_n}}(\{u,\bar{u}^n\})|=4n-6$.
\end{prop}

%AQ_n中任意两个节点最多有多少个共同节点
\begin{prop}
\cite{Ma2008}
\label{common_verticles}
Any two vertices in $AQ_n$ have at most four common neighbors for n $\geq$ 3.
\end{prop}

