\begin{prof}
\normalfont
{
Let $u$ be an arbitrary vertex in $AQ_n$. We set $H=\{x \mid (x,u)\textnormal{ is a hypercube edge}\}$ and $C=\{y \mid (y,u)\textnormal{ is a complement edge}\}$. Obviously, $|H|=n-1$ and $|C|=n$. According to the definition of $AQ_n$, property \ref{hypercube_edge} and property \ref{complement_edge}, each element in $F_n$ contains at most three distinct vertices in $N(u)$.  Namely, $\{\bar{u}^i,u^{i+1},\bar{u}^{i+1}\}$ with $1 \leq i \leq n-1$ and $\{(\bar{u}^i,u^{i+1}),(\bar{u}^i,\bar{u}^{i+1}),(u^{i+1},\bar{u}^{i+1})\} \subseteq E(AQ_n)$. Thus, the subgraph induced by $\{\bar{u}^i,u^{i+1},\bar{u}^{i+1}\}$ is isomorphic to $C_3$. We set $B=\{b_i\mid b_i\in F \textnormal{ and } \{\bar{u}^i,u^{i+1},\bar{u}^{i+1}\} \subseteq V(b_i)\}$. Since each $V(b_i)$ contains two vertices in $C$ and one vertices in $H$ and $|C|>|H|$.
Thus,  $|B|<\lfloor\frac{2n-1}{3}\rfloor$.
In the following we distinguish cases pertaining to
the value of $|B|$.\\

\indent
{
\textbf{Case 1.}\quad $|B|=0$. Since each element in $F_n$ contains at most two distinct vertices in $N(u)$. Thus, $|F_n| \geq \lceil \frac{2n-1}{2} \rceil$.
}\\\\
\indent
{
\textbf{Case 2.}\quad $|B|=1$. Suppose $\{\bar{u}^i,u^{i+1},\bar{u}^{i+1}\} \subseteq V(b_i)$ with $1 \leq i \leq n-1$ and $b_i \in B$.  Since $F_n-B$ contains at most two distinct vertices in $N(u)-V(B)$. Thus, $|F_n| \geq 1+\lceil \frac{2n-4}{2} \rceil= n-1$.
}\\\\
\indent
{
\textbf{Case 3.}\quad $|B|=2$. Suppose $\{\bar{u}^i,u^{i+1},\bar{u}^{i+1}\} \subseteq V(b_1),
\{\bar{u}^j,u^{j+1},\bar{u}^{j+1}\} \subseteq V(b_2)$ with $2 \leq i,j \leq n-1$ and $|i-j|\geq 2$.
}\\\\
\indent
 {
 \textbf{Case 3.1.}\quad $|i-j|=2$. Without loss of generality, we set $j=i+2$. Then $\{\bar{u}^j,u^{j+1},\bar{u}^{j+1}\}= \{\bar{u}^{i+2},u^{i+3},\bar{u}^{i+3}\}$. Suppose $w_1,w_2 \in N(u)-V(B)-\{u^{i+2}\}$ and $w_1 \neq w_2$. According to the definition of $AQ_n$, $N(u^{i+2}) \cap N(w_1) \cap N(w_2)=\emptyset$ and vertex $u^{i+2}$ is not adjacent to the vertex in $N(u)-V(B)$. Then there is $a$ in $F_n-B$, $u^{i+2} \in V(a)$ and $V(a) \cap \{N(u)-V(B)\} = 1$. Since the element in $F-B-a$ contains at most two distinct vertices in $N(u)-V(B)-V(a)$. Thus, $|F_n| \geq 2+1+\lceil\frac{2n-8}{2}\rceil=n-1$.
 }\\\\
 \indent
 {
 \textbf{Case 3.2.}\quad $|i-j|=4$. Without loss of generality, we set $j=i+4$. Then $\{\bar{u}^j,u^{j+1},\bar{u}^{j+1}\}= \{\bar{u}^{i+3},u^{i+4},\bar{u}^{i+4}\}$.
 %下面这句话有没有语法错误
 In the following we distinguish cases pertaining to the number of elements in $F_n-B$ containing three vertices $u^{i+2},\bar{u}^{i+2}$ and $u^{i+3}$. We denote the number of elements as $N$.
 }\\\\
\indent
 {
 \textbf{Case 3.2.1.}\quad $N=3$. There are three distinct elements in $N(u)-V(B)$ that contain one of three vertices $u^{i+2},\bar{u}^{i+2}$ and $u^{i+3}$ respectively. Suppose $w_1,w_2 \in N(u)-V(B)-\{u^{i+2},\bar{u}^{i+2}$,$u^{i+3}\}$ and $w_1 \neq w_2$. According to the definition of $AQ_n$, $N(u^{i+2}) \cap N(w_1) \cap N(w_2)=\emptyset$ and vertex $u^{i+2}$ is not adjacent to the vertex in $N(u)-V(B)-\{\bar{u}^{i+2}$,$u^{i+3}\}$. Then there is $a$ in $F_n-B$, $u^{i+2} \in V(a)$ and $V(a) \cap \{N(u)-V(B)\} = 1$. Since the element in $F_n-B-a$ contains at most two distinct vertices in $N(u)-V(B)-V(a)$. Thus, $|F_n| \geq 2+3+\lceil\frac{2n-10}{2}\rceil=n$. $\bar{u}^{i+2}$ and $u^{i+3}$ are same as $u^{i+2}$.
 }\\\\
 \indent
 {
 \textbf{Case 3.2.2.}\quad $N=2$. There are two distinct elements in $N(u)-V(B)$, one of which contains one vertex in $u^{i+2},\bar{u}^{i+2}$ and $u^{i+3}$, and the other contains the other two vertices. We assume that $a_1$ contains one vertex in $u^{i+2},\bar{u}^{i+2}$ and $u^{i+3}$ and $a_2$ contains the other two vertices.
 }\\\\
 \indent
 {
 \textbf{Case 3.2.2.1.}\quad $u^{i+2} \in V(a_1)$ and $\{\bar{u}^{i+2}, u^{i+3}\} \subseteq V(a_2)$. According to the discussion of Case 3.2.1,  $V(a_1) \cap \{N(u)-V(B)-\{\bar{u}^{i+2},u^{i+3}\}\}=1$. Vertex $\bar{u}^{i+2}$ or $u^{i+3}$ is not adjacent to the vertex in $N(u)-V(B)-\{u^{i+2}\}$. Then $\{\bar{u}^{i+2},u^{i+3}\} \subseteq a_2$ and $V(a_2) \cap \{N(u)-V(B)-\{u^{i+2}\}=2$. Since the element in $F_n-B-a_1-a_2$ contains at most two distinct vertices in $N(u)-V(B)-V(a_1)-V(a_2)$. Thus, $|F_n| \geq 2+1+1+\lceil\frac{2n-10}{2}\rceil=n-1$.  $u^{i+3} \in V(a_1)$ and $\{\bar{u}^{i+2}$ $u^{i+2}\} \subseteq V(a_2)$ is similar.
 }\\\\
 \indent
 {
 \textbf{Case 3.2.2.2.}\quad $\bar{u}^{i+2} \in V(a_1)$ and $\{u^{i+2},u^{i+3}\} \subseteq V(a_2)$. Since $\{u^{i+2},u^{i+3}\} \notin E(AQ_n)$. So this situation does not exist.
 }\\\\
 \indent
 {
 \textbf{Case 3.2.3.}\quad $N=1$. Since $\{u^{i+2},u^{i+3}\} \notin E(AQ_n)$. So this situation does not exist.
 }\\\\
 \indent
 {
 \textbf{Case 3.4.}\quad $|i-j|\geq 5$. In the following we will calculate the number of elements in $F_n-B$ containing all of vertices in $U$. Obviously, $5 \leq |U| \leq 2n-7$. Since the element in $F_n-B$ contains at most two distinct vertices in $N(u)-V(B)$ and $U$ mod $2=1$. Similar to the discussion in Case 3.2.1. $|F_n| \geq 2+1+\lceil\frac{2n-8}{2}\rceil=n-1$.
 }\\\\
 \indent
 {
 \textbf{Case 4.}\quad $|B| \geq 3$. If $b_i,b_j \in B$ and no $b_k \in B$ with $i<k<j$, we set $U_{ij}=\{u^{i+2},\bar{u}^{i+2},\ldots,u^{j-1}\}$. Obviously, $1 \leq |U| \leq 2n-7$. According to the discussion of Case 3. the minimum number of elements in $F_n-B$ that contain all vertices in $U_{ij}$ is $\lceil\frac{|U_{ij}|}{2}\rceil$. Thus, $|F_n| \geq |B|+(|B|-1)+\lceil\frac{2n-1-3*|B|-(|B|-1)}{2}\rceil=n-1$
 }

}
\end{prof}



\begin{lem}
\label{C_3>=}
For n $\geq$ 6, $\kappa(AQ_n,C_3) \geq n-1$.
\end{lem}

\begin{prof}
\normalfont
{
Let $F_n^*$ be a $C_3$-substructure set of $AQ_n$ and we will show that $AQ_n-V(F_n^*)$ is connected if $F_n^* \leq n-2$. Suppose not, Let $H$ be the smallest component of $AQ_n-V(F_n^*)$. Not that $|V(F_n^*)| \leq 3*(n-2) = 3n-6$. By lemma \ref{1_extraconnectivity}, we have $3n-6 \leq 4n-8$ for $n \geq 6$. So $|V(H)|=1$. Therefore, we assume that vertex $u \in V(H)$. By lemma \ref{min_C_3}, $|N(u) \cap V(F_n^*)| \leq 2n-2 < 2n-1$, which implies that there exists a neighbor of $u$ in $AQ_n-V(F_n^*)$. Therefore, we have $|V(H)| \geq 2$, a contradiction. Thus, $AQ_n-V(F_n^*)$ is connected. The lemma holds.

}
\end{prof}
