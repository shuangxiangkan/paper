\indent{Then, we give some properties of $AQ_n$}

\begin{thm}\cite{Choudum2002}
$\kappa(AQ_1) = 1, \kappa(AQ_2) = 3, \kappa(AQ_3) = 4,$ and for n $\geq$ 4, $\kappa(AQ_n) = 2n-1$.
\end{thm}

%K_1的结构连通度和子结构连通度
According to Theorem 1, we have the following result.
\begin{thm}
For n $\geq$ 4,$\kappa(AQ_n,K_1) = \kappa^s(AQ_n,K_1)=2n-1$.
\end{thm}

%AQ_n的连通度
\begin{lem}
\label{1_extraconnectivity}
\cite{Chang2012}
For n $\geq$ 6, $\kappa_1(AQ_n)= 4n - 8$ .
\end{lem}

%如果两个节点是hypercube edge,它们的共同的相邻节点数
\begin{prop}
\cite{Chang2012}
\label{hypercube_edge}
If $(u,u^i)$ is a hypercube edge of dimension $i$ $(2\leq i\leq n)$, then\\

\[
{N_{AQ_n}(u)}\cap {N_{AQ_n}(u^i)}=\left\{
             \begin{array}{lcr}
             \{\bar{u}^i,\bar{u}^{i-1}\} \qquad &i>1,\\
             \{\bar{u}^2,u^2\} \qquad &i=1,
             \end{array}
\right.
\]


that is, $u$ and $u^i$ have exactly two common neighbors in $AQ_n$ and $|{N_{AQ_n}}(\{u,u^i\})|= 4n-6$.
\end{prop}

%如果两个节点是complement edge,它们的共同的相邻节点数
\begin{prop}
\cite{Chang2012}
\label{complement_edge}
If (u,$\bar{u}^i$) is a complement edge of dimension i $(2\leq i\leq n-1)$, then ${N_{AQ_n}}(u)\cap {N_{AQ_n}}(\bar{u}^i)$=$\{u^i,u^{i+1},\bar{u}^{i-1},\bar{u}^{i+1}\}$, that is, $u$ and $\bar{u}^i$ have exactly four common neighbors in $AQ_n$ and $|{N_{AQ_n}}(\{u,\bar{u}^i\})|=4n-8$. If $(u,\bar{u}^n)$ is a complement edge of dimension n, then ${N_{AQ_n}}(u)\cap {N_{AQ_  n}}(\bar{u}^n)=\{\bar{u}^{n-1},u^n\}$, that is, $u$ and $\bar{u}^n$ have exactly two common neighbors in $AQ_n$ and $|{N_{AQ_n}}(\{u,\bar{u}^n\})|=4n-6$.
\end{prop}

%AQ_n中任意两个节点最多有多少个共同节点
\begin{prop}
\cite{Chang2012}
\label{common_verticles}
Any two vertices in $AQ_n$ have at most four common neighbors for n $\geq$ 3.
\end{prop}

%在其中一个AQ_n-1中的任意两个节点在另一个AQ_n-1 中有两个共同节点的条件
%这个地方的L和W还没有定义
\begin{prop}
\cite{Chang2012}
\label{neighbor_L_R}
If two vertices $u$ and $w$ in $AQ^0_{n-1}$(respectively, $AQ^1_{n-1}$) have common neighbors in $AQ^1_{n-1}$(respectively, $AQ^0_{n-1}$), then $w=\bar{u}^{n-1}$ and they have exactly two common neighbors $u^n$ and $\bar{u}^n$ in $AQ^1_{n-1}$.
\end{prop}

\begin{prop}
If $(u,u^i)$ and $(u,u^j)$ are two hypercube edges of dimension $i$ and $j$ $(1 \leq i,j\leq n)$. Without loss of generality, we set $i<j$, then

\[
{N_{AQ_n}(u^i)}\cap {N_{AQ_n}(u^j)}=\left\{
             \begin{array}{lcr}
             \{u,(u^i)^j,\bar{u}^i,(\overline{u^j})^{i-1}\}  & & j=i+1 \quad\textnormal{and}\quad i > 1,\\
             \{u,(u^i)^j\}  & & j-i>1 \quad\textnormal{or}\quad i=1,j=2
             \end{array}
\right.
\]
\end{prop}

\begin{prop}
If $(u,\bar{u}^i)$ and $(u,\bar{u}^j)$ are two complement edges of dimension $i$ and $j$ $(1 \leq i,j\leq n)$. Without loss of generality, we set $i<j$, then

\[
{N_{AQ_n}(\bar{u}^i)}\cap {N_{AQ_n}(\bar{u}^j)}=\left\{
             \begin{array}{lcr}
             \{u,u^j\}  & & j=i+1,\\
             \{u,(\overline{\bar{u}^j})^i,(\bar{u})^{j+1},(\bar{u}^i)^{i-1}\}  & & i-j=2  ,\\
             \{u,(\overline{\bar{u}^j})^i\}  & & j-i>2
             \end{array}
\right.
\]
\end{prop}

\begin{prop}
If $(u,u^i)$ is a hypercube edges of dimension $i$ and $(u,\bar{u}^j)$ is a complement edges of dimension $j$ $(1 \leq i,j\leq n)$.
then

\[
{N_{AQ_n}(u^i)}\cap {N_{AQ_n}(\bar{u}^j)}=\left\{
             \begin{array}{lcr}
             \{u,(u^i)^{i-1},(\bar{u}^j)^{j-1},\bar{u}^{j-1}\}  & & j=i,\\
             \{u,(\overline{\bar{u}^j})^i,(\bar{u})^{j+1},(\bar{u}^i)^{i-1}\}  & & i=j+1  ,\\
             \{u,(\overline{\bar{u}^j})^i\}  & & j-i>2
             \end{array}
\right.
\]
\end{prop}


\section{$H$-structure connectivity and $H$-substructure connectivity}
In this section, we study the $H$-structure connectivity and $H$-substructure connectivity of $AQ_n$ for each $H\in$ $\{K_{1,M},P_{Q},C_{N}\}$ where $1 \leq M \leq 6$, $2 \leq Q \leq 2n-1$, $3 \leq N \leq n$. We arrange all the adjacent vertices of $u$ in the following order :
$u^1,u^2,\bar{u}^{2},\ldots,u^{n},\bar{u}^{n}$ and we denote this by $v^{(1)},v^{(2)},\ldots,v^{(2n-1)}$.
%这句话的表述有没有问题
The correspondence of $u^j$ and $v^{(i)}$ is:
$(1)$if $i$ is even and $j=\frac{i}{2}+1$, $u^j=v^{(i)}$.
$(2)$if $i$ is odd and $j=\frac{i-1}{2}+1$ $(\lfloor\frac{i}{2}\rfloor+1)$, $\bar{u}^j=v^{(i)}$. For example, if $u=000000$ is a vertex of $AQ_6$, then $u^4=v^{(6)}=001000$ and $\bar{u}^5=v^{(9)}=011111$. In this paper, we may mix these two notations whenever it is convenient.

\subsection{$\kappa(AQ_n,K_{1,M})$ \textnormal{and} $\kappa^s(AQ_n,K_{1,M})$}
According to the definition of $AQ_n$, property \ref{hypercube_edge} and property \ref{complement_edge}, if $u$ is an arbitrary vertex of $AQ_n$ and $(u,\bar{u}^i)$ is a complement edge of dimension $i$ $(2\leq i \leq n-1)$, $N_{AQ_n}(u)\cap N_{AQ_n}(\bar{u}^i)=\{\bar{u}^{i-1},u^i,u^{i+1},\bar{u}^{i+1}\}$. Then the subgraph induced by $\{\bar{u}^{i-1},u^i,\bar{u}^i,u^{i+1}, \bar{u}^{i+1}\}$ $(2\leq i \leq n-1)$ is isomorphic to $K_{1,4}$. If $(u,\bar{u}^n)$ is a complement edge of dimension $n$, then $N_{AQ_n}(u)\cap N_{AQ_n}(\bar{u}^n)=\{\bar{u}^{n-1},u^n\}$ and the subgraph induced by $\{\bar{u}^{n-1},u^n$,$\bar{u}^n\}$ is isomorphic to $K_{1,2}$. Similarly, if $(u,u^i)$ is a hypercube edge of dimension $i$, the subgraph induced by $\{u^i,\bar{u}^i$,$\bar{u}^{i-1}\}$ for $2<i\leq n$ and $\{u^1,\bar{u}^2$,$u^2\}$ is isomorphic to $K_{1,2}$.

Here, we discuss in the case of $1 \leq M \leq 3$ and $4 \leq M \leq 6$.
\subsubsection{$\kappa(AQ_n,K_{1,M})$ \textnormal{and} $\kappa^s(AQ_n,K_{1,M})$ \textnormal{for} $1 \leq M \leq 3$}
\begin{lem}
\label{1<=M<=3(1)}
For n $\geq$ 4, $\kappa(AQ_n,K_{1,M})$ $\leq$ $\lceil\frac{2n-1}{1+M}\rceil$ and $\kappa^s(AQ_n,K_{1,M})$ $\leq$ $\lceil\frac{2n-1}{1+M}\rceil$.
\end{lem}

