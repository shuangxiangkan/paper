%证明4<=M<=6时，一个节点u的所有邻接节点最少能够包含的K_1_M子结构的节点数
\begin{lem}
\label{min_K_1_M}
Let $F_n$ be a $K_{1,M}$-substructure set of $AQ_n$ and $u$ be a vertex in $AQ_n - V(F_n)$. If $u$ is an isolated vertex in  $AQ_n - V(F_n)$, then $|F_n|\geq \lceil\frac{n-1}{2}\rceil$.
\end{lem}


\begin{prof}
\normalfont{
 Let $u$ be an arbitrary vertex in $AQ_n$, We set $F_n=\bigcup_{i=1}^{1+M}A_i$ where $A_i$ is a set of $\{\{x_1,x_2,\ldots,x_i\} \mid (x_1,x_j) \in E(AQ_n)$ for each $j \in \{2 \leq j \leq i\}\}$. We set $H=\{x \mid (x,u)$ is a hypercube edge$\}$ and $C=\{y \mid (y,u)\textnormal{ is a complement edge}\}$. Obviously, $|H|=n-1$ and $|C|=n$. According to the definition of $AQ_n$, property \ref{hypercube_edge}, property \ref{complement_edge} and property \ref{common_verticles}, each element in $F_n$ contains at most five distinct vertices in $N(u)$.  Namely, $\{\bar{u}^{i-1},u^i,\bar{u}^i,u^{i+1},\bar{u}^{i+1}\}$ with $2 \leq i \leq n-1$. Since $\{(\bar{u}^i,\bar{u}^{i-1}),(\bar{u}^i,u^i),(\bar{u}^i,u^{i+1}),(\bar{u}^i,\bar{u}^{i+1})\} \subseteq E(AQ_n)$, then the subgraph induced by $\{\bar{u}^{i-1},u^i,\bar{u}^i,u^{i+1},\bar{u}^{i+1}\}$ is isomorphic to $K_{1,4}$. We set $B=\{b_i\mid b_i\in F_n \textnormal{ and } \{\bar{u}^{i-1},u^i,\bar{u}^i,u^{i+1},\bar{u}^{i+1}\} \subseteq V(b_i)\}$. Since each $V(b_i)$ contains three vertices in $C$ and two vertices in $H$ and $|C|>|H|$.
  Thus,  $|B|<\lfloor\frac{2n-1}{5}\rfloor$.
  %On the other hand, any four consecutive neighbors(According to our previous agreed order) of $u$ can form a $K_{1,3}$. That is, $(1)\{u^i,\bar{u}^i,u^{i+1},\bar{u}^{i+1}\}$ for $2 \leq i \leq n-2$ and $\{(\bar{u}^i,u^i),(\bar{u}^i,u^{i+1}),(\bar{u}^i,\bar{u}^{i+1})\}\subseteq E(AQ_n)$. $(2)\{\bar{u}^i,u^{i+1},\bar{u}^{i+1},u^{i+2}\}$ for $2 \leq i \leq n-2$ and $\{(\bar{u}^{i+1},\bar{u}^i),(\bar{u}^{i+1},u^{i+1}),(\bar{u}^{i+1},u^{i+2})\}\subseteq E(AQ_n)$.
  In the following we distinguish cases pertaining to
  the value of $|B|$.\\

 \indent
 {
 \textbf{Case 1.}\quad $|B|=0$. Since each element in $F_n$ contains at most four distinct vertices in $N(u)$. Thus, $|F_n| \geq \lceil \frac{2n-1}{4} \rceil$.
 }\\\\
 \indent
 {
 \textbf{Case 2.}\quad $|B|=1$. Suppose $\{\bar{u}^{i-1},u^i,\bar{u}^i,u^{i+1},\bar{u}^{i+1}\} \subseteq V(b_i)$ with $2 \leq i \leq n-1$.  Since $F_n-B$ contains at most four distinct vertices in $N(u)-V(B)$. Thus, $|F_n| \geq 1+\lceil \frac{2n-6}{4} \rceil=\lceil \frac{n-1}{2} \rceil$.
 }\\\\
 \indent
 {
 \textbf{Case 3.}\quad $|B|=2$. Suppose $\{\bar{u}^{i-1},u^i,\bar{u}^i,u^{i+1},\bar{u}^{i+1}\} \subseteq V(b_i),
 \{\bar{u}^{j-1},u^j,\bar{u}^j,u^{j+1},\bar{u}^{j+1}\} \subseteq V(b_j)$ with $2 \leq i,j \leq n-1$ and $|i-j|\geq 3$.
 }\\\\
 \indent
 {
 \textbf{Case 3.1.}\quad $|i-j|=3$.
 Suppose $u^m,u^n \in H$ and $m<n$, if $n=m+1$ and $m \neq 1$, then $N(u^m) \cap N(u^n)=\{u,(u^m)^n,\bar{u}^m,(\overline{u^n})^{m-1}\}$. If $m=1,n=2$ or $n-m>1$, then $N(u^m) \cap N(u^n)=\{u,(u^m)^n\}$. Suppose $\bar{u}^m,\bar{u}^n \in C$, if $n=m+1$, then $N(\bar{u}^m) \cap N(\bar{u}^n)=\{u,u^n\}$. If $n-m>2$, $N(\bar{u}^m) \cap N(\bar{u}^n)=\{u,(\overline{\bar{u}^n})^m\}$. If $n-m=2$, $N(u^m) \cap N(u^n)=\{u,(\overline{\bar{u}^n})^m,(\bar{u})^{m+1},(\bar{u}^n)^{n-1}\}$


 Without loss of generality, we set $j=i+3$. Then $\{\bar{u}^{j-1},u^j,\bar{u}^j,u^{j+1},\bar{u}^{j+1}\}\\= \{\bar{u}^{i+2},u^{i+3},\bar{u}^{i+3},u^{i+4},\bar{u}^{i+4}\}$.
  Suppose $w_1,w_2 \in N(u)-V(B)-\{u^{i+2}\}$ and $w_1 \neq w_2$. According to the definition of $AQ_n$, $N(u^{i+2}) \cap N(w_1) \cap N(w_2)=\emptyset$ and vertex $u^{i+2}$ is not adjacent to the vertex in $N(u)-V(B)$. Then there is $a$ in $F_n-B$, $u^{i+2} \in V(a)$ and $V(a) \cap \{N(u)-V(B)\} \leq 2$. Since the element in $F_n-B-a$ contains at most four distinct vertices in $N(u)-V(B)-V(a)$. Thus, $|F_n| \geq 2+1+\lceil\frac{2n-13}{4}\rceil=\lceil\frac{2n-1}{4}\rceil$.
 }\\\\
 \indent
 {
 \textbf{Case 3.2.}\quad $|i-j|=4$. Without loss of generality, we set $j=i+4$. Then $\{\bar{u}^{j-1},u^j,\bar{u}^j,u^{j+1},\bar{u}^{j+1}\}\\= \{\bar{u}^{i+3},u^{i+4},\bar{u}^{i+4},u^{i+5},\bar{u}^{i+5}\}$.
 %下面这句话有没有语法错误
 In the following we distinguish cases pertaining to the number of elements in $F_n-B$ containing three vertices $u^{i+2},\bar{u}^{i+2}$ and $u^{i+3}$. We denote the number of elements as $N$.
 }\\\\
 \indent
 {
 \textbf{Case 3.2.1.}\quad $N=3$. There are three distinct elements in $N(u)-V(B)$ that contain one of three vertices $u^{i+2},\bar{u}^{i+2}$ and $u^{i+3}$, respectively. Suppose $w_1,w_2 \in N(u)-V(B)-\{u^{i+2},\bar{u}^{i+2},u^{i+3}\}$ and $w_1 \neq w_2$. According to the definition of $AQ_n$, $N(u^{i+2}) \cap N(w_1) \cap N(w_2)=\emptyset$ and vertex $u^{i+2}$ is not adjacent to the vertex in $N(u)-V(B)-\{\bar{u}^{i+2},u^{i+3}\}$. Then there is $a$ in $F_n-B$, $u^{i+2} \in V(a)$ and $V(a) \cap \{N(u)-V(B)-\{\bar{u}^{i+2},u^{i+3}\}\} \leq 2$. Since the element in $F_n-B-a$ contains at most four distinct vertices in $N(u)-V(B)-V(a)$. Thus, $|F_n| \geq 2+3+\lceil\frac{2n-17}{4}\rceil=\lceil\frac{2n+3}{4}\rceil$. $\bar{u}^{i+2}$ and $u^{i+3}$ are same as $u^{i+2}$.
 }\\\\
 \indent
 {
 \textbf{Case 3.2.2.}\quad $N=2$. There are two distinct elements in $N(u)-V(B)$, one of which contains one vertex in $u^{i+2},\bar{u}^{i+2}$ and $u^{i+3}$, and the other contains the other two vertices. We assume that $a_1$ contains one vertex in $u^{i+2},\bar{u}^{i+2}$ and $u^{i+3}$ and $a_2$ contains the other two vertices.
 %Since the element $a$ in $F_n$ contains at most two distinct vertices in $N(u)-V(B)$ and any two vertices in the $\{u^{i+2}$,$\bar{u}^{i+2},u^{i+3}\}$ are these vertices. The element in $F_n-B-a$ contains at most four distinct vertices in $N(u)-V(B)-V(a)$. Thus, $F_n \geq 2+1+1+\lceil\frac{2n-15}{4}\rceil=4+\lceil\frac{2n-15}{4}\rceil$.
 }\\\\
 \indent
 {
 \textbf{Case 3.2.2.1.}\quad $u^{i+2} \in V(a_1)$ and $\{\bar{u}^{i+2}, u^{i+3}\} \subseteq V(a_2)$. According to the discussion of Case 3.2.1, %$u^{i+2} \in V(a_1)$ and
 $V(a_1) \cap \{N(u)-V(B)-\{\bar{u}^{i+2},u^{i+3}\}\} \leq 2$. Since $N(\bar{u}^{i+2}) \cap N(u^{i+3})-\{u,\bar{u}^{i+3}\}=\{(\bar{u}^{i+2})^{i+4},(\overline{\bar{u}^{i+2}})^{i+4}\}$ and each element in $\{(\bar{u}^{i+2})^{i+4},(\overline{\bar{u}^{i+2}})^{i+4}\}$ is not adjacent to the vertices in $N(u)-V(B)-\{u^{i+2}\}$. On the other hand, each element in $\{\bar{u}^{i+2},u^{i+3}\}$ is not adjacent to the vertex in $N(u)-V(B)-\{u^{i+2}\}$. Therefore, $V(a_2) \cap \{N(u)-V(B)-\{u^{i+2}\}\}=2$ and  $\{\bar{u}^{i+2},u^{i+3}\} \subseteq V(a_2)$. Since the element in $F_n-B-a_1-a_2$ contains at most four distinct vertices in $N(u)-V(B)-V(a_1)-V(a_2)$. Thus, $|F_n| \geq 2+1+1+\lceil\frac{2n-15}{4}\rceil=\lceil \frac{2n+1}{4} \rceil$.  $u^{i+3} \in V(a_1)$ and $\{\bar{u}^{i+2}$ $u^{i+2}\} \subseteq V(a_2)$ is similar.
 }\\\\
 \indent
 {
 \textbf{Case 3.2.2.2.}\quad $\bar{u}^{i+2} \in V(a_1)$ and $\{u^{i+2},u^{i+3}\} \subseteq V(a_2)$. According to the discussion of Case 3.2.1, $V(a_1) \cap \{N(u)-V(B)-\{u^{i+2},u^{i+3}\}\} \leq 2$. Since $N(u^{i+2}) \cap N(u^{i+3})-\{u,\bar{u}^{i+2}\}=\{(u^{i+2})^{i+3},(\bar{u}^{i+3})^{i+2}\}$ and each element in $\{(u^{i+2})^{i+3},(\bar{u}^{i+3})^{i+2}\}$ is not adjacent to the vertices in $N(u)-V(B)-\{\bar{u}^{i+2}\}$. On the other hand, $(u^{i+2},u^{i+3}) \notin E(AQ_n)$. Therefore, $V(a_2) \cap \{N(u)-V(B)-\{\bar{u}^{i+2}\}\}=2$ and  $\{u^{i+2},u^{i+3}\} \subseteq V(a_2)$. Since the element in $F_n-B-a_1-a_2$ contains at most four distinct vertices in $N(u)-V(B)-V(a_1)-V(a_2)$. Thus, $|F_n| \geq 2+1+1+\lceil\frac{2n-15}{4}\rceil=\lceil \frac{2n+1}{4} \rceil$.
 }\\\\
 \indent
 {
 \textbf{Case 3.2.3.}\quad $N=1$. According to the discussion of Case 3.2.1 and Case 3.2.2, there is $a$ in $F_n-B$, $\{u^{i+2},\bar{u}^{i+2},u^{i+3}\} \subseteq V(a)$ and $V(a) \cap \{N(u)-V(B)\} = 3$. Since the element in $F_n-B-a$ contains at most four distinct vertices in $N(u)-V(B)-V(a)$. Thus, $|F_n| \geq 2+1+\lceil\frac{2n-14}{4}\rceil=\lceil \frac{n-1}{2} \rceil$.
 }\\\\
 \indent
 {
 \textbf{Case 3.4.}\quad $|i-j|\geq 5$. We set $U=\{u^{i+2},\bar{u}^{i+2},\ldots,u^{j-1}\}$. In the following we will calculate the number of elements in $F_n-B$ containing all of vertices in $U$. Since $5 \leq |U| \leq 2n-11$ and the element in $F_n-B$ contains at most four distinct vertices in $N(u)-V(B)$. We will distinguish cases pertaining to
  the value of $|U|$ mod $4$.
 }\\\\
 \indent
 {
 \textbf{Case 3.4.1.}\quad $|U|$ mod $4=1$. Similar to the discussion in Case 3.2.1. $|F_n| \geq 2+1+\lceil\frac{2n-13}{4}\rceil=\lceil\frac{2n-1}{4}\rceil$.
 }\\\\
 \indent
 {
 \textbf{Case 3.4.2.}\quad $|U|$ mod $4=2$. Similar to the discussion in Case 3.2.2. $|F_n| \geq 2+1+\lceil\frac{2n-13}{4}\rceil=\lceil\frac{2n-1}{4}\rceil$.
 }\\\\
 \indent
 {
 \textbf{Case 3.4.3.}\quad $|U|$ mod $4=3$. Similar to the discussion in Case 3.2.3. $|F_n| \geq 2+1+\lceil\frac{2n-14}{4}\rceil=\lceil\frac{n-1}{2}\rceil$.
 }\\\\
 \indent
 {
 \textbf{Case 4.}\quad $|B| \geq 3$. If $b_i,b_j \in B$ and no $b_k \in B$ with $i<k<j$, we set $U_{ij}=\{u^{i+2},\bar{u}^{i+2},\ldots,u^{j-1}\}$.  According to the discussion of Case 3. If $|U_{ij}|$ mod $4=3$. The value of $F_n$ will be the smallest. Thus, $|F_n| \geq |B|+(|B|-1)+\lceil\frac{2n-1-5*|B|-3*(|B|-1)}{4}\rceil=\lceil\frac{n-1}{2}\rceil$.
 }\\

 In summary, the lemma holds.
 }
